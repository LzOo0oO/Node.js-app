      <!-- Income 收益 -->
      <% for (var data of list) {%>
      <div class="lz-con-con-con">
        <h1><%= data.title %></h1>

        <ul>
          <li>
            <span><%= data.incomeNum %></span>
            <sub><%= data.income %></sub>
            <p>测算年化收益率</p>
          </li>
          <li>
            <p>借款期限：<span><%= data.year %>天</span></p>
            <p>还款方式：<span><%= data.yearType %></span></p>
          </li>
          <li>
            <p>筹集金额：<span><%= data.money %></span></p>
            <p>产品星级：
              <div>
                <div style="width: <%= data.star %>%;"></div>
              </div>
            </p>
          </li>
          <li>
            <p>投资进度：<span><%= data.program %>%</span><i><i  style="width: <%= data.program %>%;"></i></i></p>
            <p>可投金额：<span><%= data.mayMoney %></span></p>
          </li>
          <li>
            <div data-target= <%= data.src %>>立即加入</div>
          </li>
        </ul>
      </div>
      <% } %>


    </div>

  </div>
