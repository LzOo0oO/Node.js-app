      background: #fff;
      border-left: 3px #dd2c37 solid;
    }

    .news-box .news-left ul li {
      width: 100%;
      height: 50px;
      line-height: 50px;
      color: #333;
      border: 1px #e3e3e3 solid;
      text-align: center;
      background: #f8f8f8;
      border-left: none;
      border-top: none;
    }

    .news-box .news-right {
      background: #fff;
      display: table;
      padding: 25px 53px;
      font-size: 14px;
      height: 100%;
      /*width: 934px;*/
    }

    .n-dl {
      line-height: 28px;
      margin-bottom: 30px;
    }

    .n-dl .n-title,
    #rate-report a .h4 {
      font-size: 18px;
      font-weight: normal;
      padding: 5px 0;
    }

    .n-dl dd,
    #rate-report .h4 {
      line-height: 28px;
      font-weight: normal;
    }

    .n-dl .n-date {
      color: #6d6e70;
      font-size: 12px;
    }

    .n-dl .n-content {
      color: #6f6f6f;
    }

    .n-dl .n-more {
      color: #c2502e;
    }

    #newsHistory {
      padding-left: 70px;
    }

    .history_content {
      right: inherit;
      padding-left: 230px;
      left: inherit;
      top: 12px;
      border-left: 6px #e6eaed solid;
      position: relative;
      padding-bottom: 1px;
    }

    .history_bs {
      width: 6px;
      position: absolute;
      left: -6px;
      top: -1px;
      height: 70px;
      border-radius: 5px;
      background: #ffa412;
    }

    .history_now {
      position: absolute;
      top: 25px;
      font-size: 16px;
      left: -8%;
    }
    .history_content .history_content_right {
    margin-left: inherit;
    top: inherit;
    padding: 0 20px 0 50px;
    border-left: 5px #e6eaed solid;
    }

    .history_content_right div {
      position: relative;;
      left: inherit;
      padding-left: 130px;
    }

    .history_content_right div span {
      left: -230px;
      top: 30px;
    }

    .history_2014 {
      position: absolute;
      font-size: 18px;
    }

    .history_content_right div .his-tl {
