      left: -1%;
      position: absolute;
      color: #e6e9ee;
      font-size: 40px;
    }

    .history_content_right div img {
      position: absolute;
      left: -11%;
      top: 0px;
    }

    .history_content_right div p {
      position: absolute;
      left: -30%;
      top: 26px;
      padding: 6px 3px;
      background: #7cd1fa;
      text-align: center;
      color: #fff;
      border-radius: 4px;
      font-size: 14px;
    }

    .history_content_right div dl {
      max-width: 400px;
      background: #e6e9ee;
      border-radius: 4px;
      padding: 10px;
      margin-bottom: 50px;
      margin-left: inherit;
      right: 30%;
      top: 30px;
      position: relative;
    }

    .history_content_right div dl dt {
      line-height: 25px;
      letter-spacing: 1px;
      font-size: 18px;
      text-indent: 3px;
    }

    .history_content_right div dl dd {
      line-height: 25px;
      letter-spacing: 1px;
    }

