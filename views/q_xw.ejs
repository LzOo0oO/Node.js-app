    .news-box .news-left ul {
      margin: 0;
      padding: 0;
    }

    .news-box .news-left ul .active {

      display: block;
      position: relative;
      left: -2px;

      cursor: pointer;
      height: 50px;
      border-right: none;
    }

    .show > div {
      display: none;
    }

    .show > div.active {
      display: block;
    }

    .news-box .news-left ul li.active,
