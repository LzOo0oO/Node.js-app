  <title></title>
  <style media="screen">
    * {
      margin: 0;
      padding: 0;
    }

    .new-bg {
      background-color: #f3f3f3;
      padding: 30px 0;
      /*height: 1000px;*/
    }

    .news-box {
      background: #f3f3f3;
      border: 1px #e3e3e3 solid;
      width: 1200px;
      margin: 0 auto;
      /*height: 1000px;*/
    }

    .news-box .news-left {
      float: left;
      width: 18%;
    }

    .news-box .news-left ul {
      margin: 0;
      padding: 0;
    }

    .news-box .news-left ul .active {

      display: block;
      position: relative;
      left: -2px;

      cursor: pointer;
      height: 50px;
      border-right: none;
    }

    .show > div {
      display: none;
    }

    .show > div.active {
      display: block;
    }

    .news-box .news-left ul li.active,
    .news-box .news-left.q ul li:hover {
      background: #fff;
      border-left: 3px #dd2c37 solid;
    }

    .news-box .news-left ul li {
      width: 100%;
      height: 50px;
      line-height: 50px;
      color: #333;
      border: 1px #e3e3e3 solid;
      text-align: center;
      background: #f8f8f8;
      border-left: none;
      border-top: none;
    }

    .news-box .news-right {
      background: #fff;
      display: table;
      padding: 25px 88px;
      font-size: 14px;
      height: 100%;
      /*width: 934px;*/
    }

    .n-dl {
      line-height: 28px;
      margin-bottom: 30px;
    }

    .n-dl .n-title,
    #rate-report a .h4 {
      font-size: 18px;
      font-weight: normal;
      padding: 5px 0;
    }

    .n-dl dd,
    #rate-report .h4 {
      line-height: 28px;
      font-weight: normal;
    }

    .n-dl .n-date {
      color: #6d6e70;
      font-size: 12px;
    }

    .n-dl .n-content {
      color: #6f6f6f;
    }

    .n-dl .n-more {
      color: #c2502e;
    }

    #newsHistory {
      padding-left: 70px;
    }

    .history_content {
      right: inherit;
      padding-left: 230px;
      left: inherit;
      top: 12px;
      border-left: 6px #e6eaed solid;
      position: relative;
      padding-bottom: 1px;
    }

    .history_bs {
      width: 6px;
      position: absolute;
      left: -6px;
      top: -1px;
      height: 70px;
      border-radius: 5px;
      background: #ffa412;
    }

    .history_now {
      position: absolute;
      top: 25px;
      font-size: 16px;
      left: -8%;
    }
    .history_content .history_content_right {
    margin-left: inherit;
    top: inherit;
    padding: 0 20px 0 50px;
    border-left: 5px #e6eaed solid;
    }

    .history_content_right div {
      position: relative;;
      left: inherit;
      padding-left: 130px;
    }

    .history_content_right div span {
      left: -230px;
      top: 30px;
    }

    .history_2014 {
      position: absolute;
      font-size: 18px;
    }

    .history_content_right div .his-tl {
      top: 16px;
      left: -1%;
      position: absolute;
      color: #e6e9ee;
      font-size: 40px;
    }

    .history_content_right div img {
      position: absolute;
      left: -12%;
      top: 0px;
    }

    .history_content_right div p {
      position: absolute;
      left: -30%;
      top: 26px;
      padding: 6px 3px;
      background: #7cd1fa;
      text-align: center;
      color: #fff;
      border-radius: 4px;
      font-size: 14px;
    }

    .history_content_right div dl {
      max-width: 400px;
      background: #e6e9ee;
      border-radius: 4px;
      padding: 10px;
      margin-bottom: 50px;
      margin-left: inherit;
      right: 30%;
      top: 30px;
      position: relative;
    }

    .history_content_right div dl dt {
      line-height: 25px;
      letter-spacing: 1px;
      font-size: 18px;
      text-indent: 3px;
    }

    .history_content_right div dl dd {
      line-height: 25px;
      letter-spacing: 1px;
    }

    .report-box .h4 {
    font-size: 18px;
    color: #222;
    padding-bottom: 25px;
    }
    .report-box .report-content {
    color: #666;
    line-height: 28px;
    text-indent: 30px;
    margin-bottom: 30px;
    }
    .report-box .report-list li {
    color: #444;
    height: 2px;
    line-height: 43px;
    padding: 12px 50px 45px;
    background: url("images/rate.jpg") no-repeat 6px 13px;
    border-bottom: 1px solid #c6cbcf;
    }





  </style>
</head>

